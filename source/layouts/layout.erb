<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= data.page.title || "New Ruins" %></title>
    
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
    
    
  <body class="<%= page_classes %>" style= "background-image:url(/images/Umm_Rasas_House_ruins.JPG)">
    <img src="/images/NewRuinLogo.png", alt:"New Ruins"><img>
    <%= partial "layouts/nav" %>
    <%= yield %>
  </body>
</html>